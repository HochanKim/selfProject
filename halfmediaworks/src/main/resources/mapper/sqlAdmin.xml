<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.hmw.admin.mapper.UserMapper">	
	<!-- 관리자 페이지 사용자 리스트 가져오기-->
	<select id="getUser" parameterType="hashmap" resultType="com.example.hmw.admin.model.UserModel">	
		<!-- 'resultType'에 잘못된 작성으로 오류가 발생했음 -->
		SELECT *
		FROM HMW_USER
	</select>
</mapper>